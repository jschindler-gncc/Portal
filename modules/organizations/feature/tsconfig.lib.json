{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "../../dist/out-tsc",
    "declaration": true,
    "declarationMap": true,
    "inlineSources": true,
    "types": []
  },
  "exclude": [
    "feature/src/**/*.spec.ts",
    "feature/src/test-setup.ts",
    "feature/jest.config.ts",
    "feature/src/**/*.test.ts"
  ],
  "include": ["feature/src/**/*.ts"]
}
